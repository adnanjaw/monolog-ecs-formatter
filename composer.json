{
    "name": "pimjansen/monolog-ecs-formatter",
    "description": "Monolog formatter for Elastic Common Schema",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "monolog/monolog": "^2.2",
        "ext-json": "*"
    },
    "require-dev": {
        "twig/twig": "^3.3",
        "symfony/yaml": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "ECS\\Formatter\\": "src/",
            "ECS\\Generator\\": "generator/"
        }
    },
    "config": {
        "sort-packages": true
    }

}
